{
  "name": "Gitflow Branches",
  "target": "branch",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "include": [
        "refs/heads/hotfix/*",
        "refs/heads/feature/*",
        "refs/heads/features/*",
        "refs/heads/release/*"
      ],
      "exclude": []
    }
  },
  "rules": [
    {
      "type": "update",
      "parameters": {
        "update_allows_fetch_and_merge": true
      }
    }
  ],
  "bypass_actors": []
}
